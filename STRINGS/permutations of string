class Solution
{
	public:
		vector<string>find_permutation(string S)
		{
          
          vector<string>rans;
		   sort(S.begin(), S.end());
        
            do {
                rans.push_back(S);
                } 
            while (next_permutation(S.begin(), S.end()));
            
		   return rans; 
		}
		};
