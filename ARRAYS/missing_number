

// User function Template for Java

class Solution {
    int MissingNumber(int array[], int n) {
        
        int a=0;
        int b=0;
        
        
        for(int i=1;i<=n;i++)
            a=a^i;
        
        for(int i=0;i<n-1;i++)
            b=b^array[i];
            
        return a^b;
   
        
